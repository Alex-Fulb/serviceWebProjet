<!DOCTYPE html>
<html>

<head>
  <link rel="stylesheet" type="text/css" href="/css/botstyle.css" />
  <link rel="stylesheet" type="text/css" href="/css/dropdown.css" />
  <%- include('../partials/head'); %>
    <link rel="stylesheet" type="text/css" href="/css/teststyle.css" />
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css"
      integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
</head>

<header>
  <%- include('../partials/header'); %>
</header>

<body style="margin: 0 auto; text-align: center">
  <div>
  </div>
  <!--START-->
  <div class="row">
    <div class="chatbox chatbox22 chatbox--tray" style="border: 1px solid;">
      <!--Header-->
      <div class="chatbox__title">
        <h5 class="test"><a href="javascript:void()" style="user-select: none;">FAQBot</a></h5>
        <!---->
          <div class="dropdown dropdown--scrollable">
            <button onclick="clickDropdown()" class="dropbtn">+</button>
            <div id="myDropdown" class="dropdown-content" style="right: 0;">
              <input type="text" placeholder="Search.." id="myInput" onkeyup="filterFunction()">
              <a onclick="changeBrain('eng');clickDropdown()">English</a>
              <a onclick="changeBrain('fr');clickDropdown()">Français</a>
              <a onclick="changeBrain('spa');clickDropdown()">Español</a>
            </div>
          </div>
          <!---->
      </div>
      <!---->
      <!--TextBox-->
      <div class="chatbox__body">
        <ul class="messages"></ul>
      </div>
      <!--TextBox End-->
      <!--footer-->
      <div class="panel-footer">
        <div class="input-group">
          <form>
            <input type="submit" class="btn btn-warning btn-sm" style="float: right;" />
            <div style="overflow: hidden; padding-right: .5em;">
              <input id="inputChat" type="text" class="form-control input-sm" placeholder="Écrivez ici"
                autocomplete="off">
            </div>
          </form>
        </div>
      </div>
      <!---->
    </div>
  </div>
  <!--END-->

  <script src="https://unpkg.com/rivescript@latest/dist/rivescript.min.js"></script>
  <script src="/js/faqbotscript.js"></script>

</body>
<script src="https://unpkg.com/rivescript@latest/dist/rivescript.min.js"></script>
